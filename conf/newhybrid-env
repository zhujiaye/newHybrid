//This is the file named all the environment variables used in newhybrid
[NewHybrid]
	#newhybrid.usemysql true
	#newhybrid.usevoltdb true
	#newhybrid.initdb voltdb
	newhybrid.initdb voltdb
[Server]
	//server address must be defined
	newhybrid.server.address 10.20.2.28 
	#newhybrid.server.timeout 30000000000
	#newhybrid.server.port 12345
	#newhybrid.server.model.isdeterministic false
	#newhybrid.sever.usememmonitor true
[MySQL server]
	//mysql server address must be defined
	newhybrid.mysql.server.address 10.20.2.28
	//mysql server db name must be defined
	newhybrid.mysql.server.dbname tpcc3000
	#newhybrid.mysql.server.username remote
	#newhybrid.mysql.server.password remote
	//be careful with the folder name in different OS
	#newhybrid.mysql.tempfolder /tmpd
	#newhybrid.mysql.pool.initsize 40
[VoltDB server]
	//voltdb server address must be defined
	newhybrid.voltdb.server.address 10.20.2.28
	//voltdb server capacity must be defined in units of MB
	newhybrid.voltdb.server.capacity 14
	#newhybrid.voltdb.pool.initsize 0
[For test]
	//if test the workload file's path must be defined
	newhybrid.test.workloadfile.path load.txt
